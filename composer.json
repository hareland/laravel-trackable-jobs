{
  "name": "hareland/laravel-trackable-jobs",
  "description": "A simple solution for tracking Laravel jobs.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Hareland\\Trackable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hareland\\Trackable\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "hareland",
      "email": "packagist@protonmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.3",
    "orchestra/testbench": "^7.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hareland\\Trackable\\LaravelTrackableJobServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
